{
  "name": "browser-extension",
  "version": "3.5.12",
  "description": "AdGuard Extension",
  "scripts": {
    "dev": "BUILD_ENV=dev babel-node tools/bundle.js",
    "beta": "BUILD_ENV=beta babel-node tools/bundle.js",
    "release": "BUILD_ENV=release babel-node tools/bundle.js",
    "sample-api": "BUILD_ENV=dev gulp buildSampleApi --require babel-register --gulpfile tasks",
    "test": "gulp runTests --require babel-register --gulpfile tasks",
    "locales:download": "babel-node tools/locales.js download",
    "locales:upload": "babel-node tools/locales.js upload",
    "locales:renew": "babel-node tools/locales.js renew",
    "resources": "babel-node tools/resources.js",
    "lint": "eslint .",
    "increment": "yarn version --patch --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:AdguardTeam/AdguardBrowserExtension.git"
  },
  "author": "adguard@adguard.com",
  "license": "LGPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "adguard-assistant": "https://github.com/AdguardTeam/AdguardAssistant.git#4.3.32",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "checksum": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "^6.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cpy": "^8.1.0",
    "create-file-webpack": "^1.0.2",
    "crx": "^5.0.1",
    "del": "^4.1.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "form-data": "^2.5.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-download2": "^1.0.2",
    "gulp-file": "^0.4.0",
    "gulp-hash-creator": "^0.2.0",
    "gulp-modify-file": "^1.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^4.1.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.11",
    "node-qunit-puppeteer": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "preprocess": "^3.2.0",
    "punycode": "^2.1.0",
    "qunit": "^2.6.0",
    "scriptlets": "https://github.com/AdguardTeam/Scriptlets.git#v1.3.0",
    "web-ext": "^2.6.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2"
  },
  "dependencies": {
    "@adguard/tsurlfilter": "^0.2.0",
    "@babel/runtime": "^7.11.2",
    "crypto-js": "^4.0.0",
    "simply-deferred": "^3.0.0"
  }
}
