.editor {
    width: 100%;
    height: 300px;
    min-width: 100%;
    min-height: 300px;
    max-width: 900px;
    max-height: 100%;
    box-shadow: inset -10px 0 0 0 var(--gray05);
    position: relative;
    overflow: hidden;
    padding-right: 10px;

    & .ace_editor {
        width: 100%;
        height: 100%;
        border-bottom: 1px solid var(--gray05);
    }
}
