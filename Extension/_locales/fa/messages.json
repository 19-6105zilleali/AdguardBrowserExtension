    "sb_adguard_blocked": {
        "message": "AdGuard \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0647 \u0627\u06cc\u0646 \u0635\u0641\u062d\u0647 \u0631\u0627 \u0645\u0633\u062f\u0648\u062f \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a&nbsp;"
    },
    "sb_malware_description": {
        "message": "AdGuard \u0646\u0631\u0645 \u0627\u0641\u0632\u0627\u0631 \u0645\u062e\u0631\u0628\u06cc \u0631\u0627 \u06cc\u0627\u0641\u062a \u06a9\u0647 \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u062f\u0631 \u0631\u0627\u06cc\u0627\u0646\u0647 \u0634\u0645\u0627 \u0646\u0635\u0628 \u0634\u0648\u062f \u0627\u06af\u0631 \u0634\u0645\u0627 \u0627\u062f\u0627\u0645\u0647 \u062f\u0647\u06cc\u062f.\u0627\u06af\u0631 \u0634\u0645\u0627 \u0627\u0632 \u0627\u06cc\u0646 \u0633\u0627\u06cc\u062a \u062f\u0631 \u06af\u0630\u0634\u062a\u0647 \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0631\u062f\u0647 \u0627\u06cc\u062f \u06cc\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u0633\u0627\u06cc\u062a \u0627\u0639\u062a\u0645\u0627\u062f \u062f\u0627\u0631\u06cc\u062f\u060c\u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u0627\u062e\u06cc\u0631\u0627 \u062a\u0648\u0633\u0637 \u06cc\u06a9 \u0647\u06a9\u0631 \u062f\u0631 \u0645\u0639\u0631\u0636 \u062e\u0637 \u0642\u0631\u0627\u0631 \u06af\u0631\u0641\u062a\u0647 \u0628\u0627\u0634\u062f.\u0645\u0627 \u062a\u0648\u0635\u06cc\u0647 \u0645\u06cc\u06a9\u0646\u06cc\u0645 \u0627\u062f\u0627\u0645\u0647 \u0646\u062f\u0647\u06cc\u062f \u06cc\u0627 \u0628\u0639\u062f\u0627 \u062f\u0648\u0628\u0627\u0631\u0647 \u0627\u0645\u062a\u062d\u0627\u0646 \u06a9\u0646\u06cc\u062f."
    },
    "sb_phishing_description": {
        "message": "AdGuard \u0645\u062a\u0648\u062c\u0647 \u0634\u062f\u0647 \u06a9\u0647 \u0627\u06cc\u0646 \u0635\u0641\u062d\u0647 \u0648\u0628 \u0634\u0627\u06cc\u062f \u0646\u0633\u062e\u0647 \u062c\u0639\u0644\u06cc \u06cc\u0627 \u0628\u062f\u0644\u06cc \u06cc\u06a9 \u0633\u0627\u06cc\u062a \u062f\u06cc\u06af\u0631 \u0628\u0627\u0634\u062f\u060c\u06a9\u0647 \u0628\u0631\u0627\u06cc \u06af\u0648\u0644 \u0632\u062f\u0646 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0628\u0631\u0627\u06cc \u0628\u0647 \u0627\u0634\u062a\u0631\u0627\u06a9 \u06af\u0630\u0627\u0631\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0634\u062e\u0635\u06cc \u06cc\u0627 \u0645\u0627\u0644\u06cc \u0637\u0631\u0627\u062d\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a.\u0648\u0627\u0631\u062f \u06a9\u0631\u062f\u0646 \u0647\u0631\u06af\u0648\u0646\u0647 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0634\u062e\u0635\u06cc \u062f\u0631 \u0627\u06cc\u0646 \u0635\u0641\u062d\u0647 \u0645\u06cc\u062a\u0648\u0627\u0646\u062f \u0645\u0648\u062c\u0628 \u0633\u0631\u0642\u062a \u0647\u0648\u06cc\u062a \u06cc\u0627 \u0633\u0627\u06cc\u0631 \u0633\u0648\u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0647\u0627 \u0634\u0648\u062f."
